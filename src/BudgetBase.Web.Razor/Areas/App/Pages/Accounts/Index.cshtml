@page
@model BudgetBase.Web.Razor.Areas.App.Pages.Accounts.AccountIndexModel

@{
    ViewData["Title"] = "Accounts";
}

<div class="d-md-flex justify-content-between align-items-center">
    <h5 class="mb-0 d-none d-lg-block">@ViewData["Title"]</h5>

    @await Component.InvokeAsync("Breadcrumb", new { breadcrumbModels = Model.Breadcrumbs })
</div>
<div class="row">
    <div class="col-md-12 mt-4">
        <div class="card rounded">
            @await Component.InvokeAsync("Tabulator", new { ajaxUrl = "Accounts?handler=RemoteData" })
        </div>
    </div>
</div>

